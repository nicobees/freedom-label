                  },
                  "ax": {
                    "type": "string",
                    "pattern": "^\\d{1,3}$"
                  },
                  "batch": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "not": {}
                          },
                          {
                            "type": "string",
                            "pattern": "^.{0,7}$"
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "bc": {
                    "type": "string",
                    "pattern": "^\\d{1,2}\\.\\d{2}$"
                  },
                  "bc_toric": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "not": {}
                          },
                          {
                            "type": "string",
                            "pattern": "^\\d{1,2}\\.\\d{2}$"
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "cyl": {
                    "type": "string",
                    "pattern": "^[+-]?\\d{1,2}\\.\\d{2}$"
                  },
                  "dia": {
                    "type": "string",
                    "pattern": "^\\d{1,2}\\.\\d{2}$"
                  },
                  "pwr": {
                    "type": "string",
                    "pattern": "^[+-]?\\d{1,2}\\.\\d{2}$"
                  },
                  "sag": {
                    "type": "string",
                    "pattern": "^\\d{1,4}$"
                  },
                  "sag_toric": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "not": {}
                          },
                          {
                            "type": "string",
                            "pattern": "^\\d{1,4}$"
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "required": ["add", "ax", "bc", "cyl", "dia", "pwr", "sag"],
                "additionalProperties": false
              },
              "enabled": {
                "type": "boolean",
                "const": true
              }
            },
            "required": ["data", "enabled"],
            "additionalProperties": false
          },
          {
            "type": "object",
            "properties": {
              "data": {
                "anyOf": [
                  {},
                  {
                    "type": "null"
                  }
                ]
              },
              "enabled": {
                "type": "boolean",
                "const": false
              }
            },
            "required": ["enabled"],
            "additionalProperties": false
          }
        ]
