      "right": {
        "$ref": "#/definitions/LabelDataSchema/properties/lens_specs/properties/left"
      }
    },
    "required": ["left", "right"],
    "additionalProperties": false
  },
  "patient_info": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 2,
        "maxLength": 30
