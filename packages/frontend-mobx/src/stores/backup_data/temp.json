      "surname": {
        "type": "string",
        "minLength": 2,
        "maxLength": 30
      }
    },
    "required": ["name", "surname"],
    "additionalProperties": false
  },
  "production_date": {
    "type": "string",
    "pattern": "^\\d{2}\\/\\d{2}\\/\\d{4}$"
  }
